/dts-v1/;
#include "mt6779.dtsi"

/ {
	model = "MediaTek MT6779 EVB";
	compatible = "mediatek,mt6779-evb", "mediatek,mt6779";

	aliases {
		serial0 = &uart0;
	};

	memory@40000000 {
		device_type = "memory";
		reg = <0 0x40000000 0 0x1e800000>;
	};

	chosen {
		stdout-path = "serial0:921600n8";
	};
};

&uart0 {
	status = "okay";
};

&dramc {
	dram_type = <0>;
	channel_cnt = <2>;
	rank_cnt = <2>;
	rank_size = <0x0 0x0>;
	mr_cnt = <1>;
	mr = <0x5 0xff>;
};
