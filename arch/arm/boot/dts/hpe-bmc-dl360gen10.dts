/*
 * Device Tree file for HPE DL360Gen10
 */

/include/ "hpe-gxp.dtsi"

/ {
	#address-cells = <1>;
	#size-cells = <1>;
	compatible = "hpe,gxp";
	model = "Hewlett Packard Enterprise ProLiant dl360 Gen10";

	chosen {
		bootargs = "earlyprintk console=ttyS0,115200 user_debug=31";
	};

	memory@40000000 {
		device_type = "memory";
		reg = <0x40000000 0x20000000>;
	};

	ahb {

	};

};
